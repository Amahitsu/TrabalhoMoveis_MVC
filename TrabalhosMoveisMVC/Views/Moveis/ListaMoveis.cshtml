@model List<ItemModelView>
}
<h3 class="fw-bolder"> Lista de móveis </h3>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Nome do Móvel</th>
            <th scope="col">Check</th>
        </tr>
    </thead>
    <tbody>
        @{
            var cont = 1;
        }
        @foreach (var item in Model)
        {
            <tr>
                <td>@cont</td>
                <td>@item.nomeMoveis</td>
                <td>@item.checkMoveis</td>
            </tr>
            cont++;
            <div class="form-check">
                <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
                <label class="form-check-label" for="flexCheckDefault">
                    Default checkbox
                </label>
            </div>

        }

    </tbody>
</table>
